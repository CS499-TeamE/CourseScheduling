<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="564.0" stylesheets="@darktheme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FinalizeInputController">
   <children>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="599.0">
         <children>
            <ComboBox fx:id="courseComboBox" layoutX="15.0" layoutY="153.0" onAction="#courseValueChosen" prefHeight="25.0" prefWidth="155.0" promptText="Courses" />
            <ComboBox fx:id="professorComboBox" layoutX="14.0" layoutY="241.0" onAction="#professorValueChosen" prefHeight="25.0" prefWidth="155.0" promptText="Faculty">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ComboBox>
            <ComboBox fx:id="roomComboBox" layoutX="15.0" layoutY="316.0" onAction="#roomValueChosen" prefHeight="25.0" prefWidth="155.0" promptText="Rooms" />
            <Label layoutX="111.0" layoutY="14.0" prefHeight="54.0" prefWidth="401.0" text="Department Editor">
               <font>
                  <Font name="System Bold" size="37.0" />
               </font>
               <opaqueInsets>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </opaqueInsets>
            </Label>
            <Button fx:id="courseAdd" layoutX="175.0" layoutY="153.0" mnemonicParsing="false" onAction="#addCourse" prefHeight="25.0" text="+" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="professorAdd" layoutX="176.0" layoutY="241.0" mnemonicParsing="false" onAction="#addProfessor" prefHeight="25.0" prefWidth="24.0" text="+" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="11.0" />
               </font>
            </Button>
            <Button fx:id="roomAdd" layoutX="175.0" layoutY="316.0" mnemonicParsing="false" onAction="#addRoom" prefHeight="25.0" text="+" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <ComboBox fx:id="departmentComboBox" layoutX="15.0" layoutY="76.0" onAction="#updateDepartmentValues" prefHeight="25.0" prefWidth="155.0" promptText="Department" />
            <Button fx:id="courseEdit" layoutX="221.0" layoutY="153.0" mnemonicParsing="false" onAction="#editCourse" prefHeight="25.0" prefWidth="51.0" text="Edit" />
            <Button fx:id="courseDelete" layoutX="286.0" layoutY="153.0" mnemonicParsing="false" onAction="#deleteCourse" text="Delete" />
            <Button fx:id="professorEdit" layoutX="221.0" layoutY="241.0" mnemonicParsing="false" onAction="#editProfessor" prefHeight="25.0" prefWidth="51.0" text="Edit" />
            <Button fx:id="professorDelete" layoutX="286.0" layoutY="241.0" mnemonicParsing="false" onAction="#deleteProfessor" text="Delete" />
            <Button fx:id="roomEdit" layoutX="221.0" layoutY="316.0" mnemonicParsing="false" onAction="#editRoom" prefHeight="25.0" prefWidth="51.0" text="Edit" />
            <Button fx:id="roomDelete" layoutX="286.0" layoutY="316.0" mnemonicParsing="false" onAction="#deleteRoom" text="Delete" />
            <Button fx:id="submit" layoutX="439.0" layoutY="350.0" mnemonicParsing="false" onAction="#submit" prefHeight="25.0" prefWidth="104.0" text="Submit" />
            <Label layoutX="15.0" layoutY="287.0" text="Rooms:">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <Label layoutX="15.0" layoutY="213.0" text="Faculty:">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="127.0" text="Courses:">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <Button fx:id="backButton" layoutX="16.0" layoutY="362.0" mnemonicParsing="false" onAction="#back" text="Back" />
            <Label fx:id="statusLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="386.0" layoutY="270.0" prefHeight="54.0" prefWidth="243.0" textFill="WHITE">
               <font>
                  <Font size="19.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
