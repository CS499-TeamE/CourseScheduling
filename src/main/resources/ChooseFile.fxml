<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" stylesheets="@darktheme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ChooseFileController">
   <children>
      <BorderPane>
         <bottom>
            <ButtonBar prefHeight="45.0" prefWidth="419.0" BorderPane.alignment="CENTER">
              <buttons>
                  <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submit" text="Submit" translateX="-5.0" translateY="-5.0" />
                  <Button fx:id="addInputButton" layoutX="292.0" layoutY="20.0" mnemonicParsing="false" onAction="#addInput" text="Add Input" translateX="-5.0" translateY="-5.0" />
              </buttons>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </ButtonBar>
         </bottom>
         <center>
            <VBox fx:id="vBoxMid" BorderPane.alignment="CENTER">
               <children>
                  <HBox fx:id="hBoxTop" spacing="20.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Enter Location of File Path to Input File">
                           <font>
                              <Font size="13.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
